{
  "name": "swim-kochi-org",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "npx eslint --fix src/ --ignore-pattern 'src/app/generated/**/*'",
    "format": "npx prettier --write src && npx prisma format",
    "reset": "rm -rf ./prisma/migrations && prisma migrate reset",
    "push": "rm -rf ./prisma/migrations && mkdir -p ./prisma/migrations/$(date '+%Y%m%d')_baseline && npx prisma migrate diff --from-schema-datamodel ./prisma/schema.prisma --to-schema-datamodel ./prisma/schema.prisma --script > ./prisma/migrations/$(date '+%Y%m%d')_baseline/migration.sql && npx prisma migrate resolve --applied $(date '+%Y%m%d')_baseline && npx prisma migrate deploy",
    "migrate": "npx prisma migrate dev --name init",
    "seed": "npx prisma db seed",
    "backup": "eval $(grep '^DATABASE_URL' .env) && pg_dump -Fc -d $DATABASE_URL > backup_$(date '+%Y%m%d').dump",
    "restore": "eval $(grep '^DATABASE_URL' .env) && pg_restore --data-only -d $DATABASE_URL backup.dump"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@headlessui/react": "^2.2.9",
